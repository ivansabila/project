<div class="sidebar border-r border-grey bg-white flex flex-col justify-between w-72 h-full pt-14 fixed top-0 left-0">
    <div class="flex flex-col p-5">
        <a class="itemSidebar flex gap-4 items-center mb-1" href="/">
            <div class="flex justify-center items-center <%= data.active === "dashboard" ? "bg-darkBlue" : "bg-white" %>
                p-2">
                <svg class="<%= data.active === "dashboard" ? "stroke-white" : "stroke-secBlue" %>"
                    xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none"
                    stroke="#999999" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                    class="lucide lucide-house-icon lucide-house">
                    <path d="M15 21v-8a1 1 0 0 0-1-1h-4a1 1 0 0 0-1 1v8" />
                    <path
                        d="M3 10a2 2 0 0 1 .709-1.528l7-5.999a2 2 0 0 1 2.582 0l7 5.999A2 2 0 0 1 21 10v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z" />
                </svg>
            </div>
            <h1 class="font-base <%= data.active === "dashboard" ? "text-darkBlue font-bold" : "text-secBlue" %>">Dashboard
            </h1>
        </a>
        <a class="itemSidebar flex gap-4 items-center mb-1" href="/activity">
            <div class="flex justify-center items-center bg-darkBlue p-2 <%= data.active === "activity" ? "bg-darkBlue" : "bg-white" %>">
                <svg class="<%= data.active === "activity" ? "stroke-white" : "stroke-secBlue" %>" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="#999999"
                    stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                    class="lucide lucide-book-open-text-icon lucide-book-open-text">
                    <path d="M12 7v14" />
                    <path d="M16 12h2" />
                    <path d="M16 8h2" />
                    <path
                        d="M3 18a1 1 0 0 1-1-1V4a1 1 0 0 1 1-1h5a4 4 0 0 1 4 4 4 4 0 0 1 4-4h5a1 1 0 0 1 1 1v13a1 1 0 0 1-1 1h-6a3 3 0 0 0-3 3 3 3 0 0 0-3-3z" />
                    <path d="M6 12h2" />
                    <path d="M6 8h2" />
                </svg>
            </div>
            <h1 class="font-base <%= data.active === "activity" ? "text-darkBlue font-bold" : "text-secBlue" %>">Kegiatan</h1>
        </a>
        <a class="itemSidebar flex gap-4 items-center mb-1" href="/schedule">
            <div class="flex justify-center items-center bg-darkBlue p-2 <%= data.active === "schedule" ? "bg-darkBlue" : "bg-white" %>">
                <svg class="<%= data.active === "schedule" ? "stroke-white" : "stroke-secBlue" %>" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="#999999"
                    stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="lucide lucide-calendar-icon lucide-calendar">
                    <path d="M8 2v4" />
                    <path d="M16 2v4" />
                    <rect width="18" height="18" x="3" y="4" rx="2" />
                    <path d="M3 10h18" />
                </svg>
            </div>
            <h1 class="font-base <%= data.active === "schedule" ? "text-darkBlue font-bold" : "text-secBlue" %>">Jadwal</h1>
        </a>
        <a class="itemSidebar flex gap-4 items-center mb-1" href="/users">
            <div class="flex justify-center items-center bg-darkBlue p-2 <%= data.active === "users" ? "bg-darkBlue" : "bg-white" %>">
                <svg class="<%= data.active === "users" ? "stroke-white" : "stroke-secBlue" %>" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="#999999"
                    stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                    class="lucide lucide-users-round-icon lucide-users-round">
                    <path d="M18 21a8 8 0 0 0-16 0" />
                    <circle cx="10" cy="8" r="5" />
                    <path d="M22 20c0-3.37-2-6.5-4-8a5 5 0 0 0-.45-8.3" />
                </svg>
            </div>
            <h1 class="font-base <%= data.active === "users" ? "text-darkBlue font-bold" : "text-secBlue" %>">Pengguna</h1>
        </a>
    </div>
    <a class="itemSidebar p-7 flex gap-4 rounded-lg items-center" href="/logout">
        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="#999999"
            stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
            class="lucide lucide-log-out-icon lucide-log-out">
            <path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4" />
            <polyline points="16 17 21 12 16 7" />
            <line x1="21" x2="9" y1="12" y2="12" />
        </svg>
        <h1 class="font-base text-secBlue">Logout</h1>
    </a>

</div>